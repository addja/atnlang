def int(a) {
	r = 0;
	for (i = 0; i < a.length; i=i+1) {
		r = r*10;
		if (a[i] == "1") r = r + 1;
		else if (a[i] == "2") r = r + 2;
		else if (a[i] == "3") r = r + 3;
		else if (a[i] == "4") r = r + 4;
		else if (a[i] == "5") r = r + 5;
		else if (a[i] == "6") r = r + 6;
		else if (a[i] == "7") r = r + 7;
		else if (a[i] == "8") r = r + 8;
		else if (a[i] == "9") r = r + 9;
	}
	return r;
}


